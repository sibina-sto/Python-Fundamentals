# 04-03. FUNCTIONS [More Exercises]
# 05. Multiplication Sign

def multiplication_sign(num1, num2, num3):
    list_num = [num1, num2, num3]
    count_neg = 0
    if 0 in list_num:
        print('zero')
    else:
        for i in list_num:
            if i < 0:
                count_neg += 1
        if count_neg % 2 == 0:
            print('positive')
        else:
            print('negative')


multiplication_sign(int(input()), int(input()), int(input()))
